{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
		"repositories": [
    {
        "type":"package",
        "package": {
          "name": "clbustos/PHP_Beautifier",
          "version":"master",
          "source": {
              "url": "https://github.com/clbustos/PHP_Beautifier.git",
              "type": "git",
              "reference":"master"
            }
        }
    }
    ],
	"require": {
		"laravel/framework": "~4.1.28",
		"creolab/laravel-modules": "dev-master@dev",
		"fzaninotto/faker": "1.3.*",
		"vespakoen/menu": "dev-master",
		"zizaco/confide": "3.2.x",
		"zizaco/entrust": "~1.2",
		"bllim/datatables": "~1.3",
		"laravelbook/ardent": "dev-master",
		"maatwebsite/excel": "dev-master",
		"orangehill/iseed": "dev-master",
		"barryvdh/laravel-dompdf": "*",
		"dompdf/dompdf" : "0.6.*",
		"clbustos/PHP_Beautifier": "dev-master"
	},
	"require-dev": {
		"itsgoingd/clockwork": "dev-master",
		"way/generators": "~2",
		"phpunit/phpunit": "~4.0",
		"mockery/mockery": "~0.9",
		"summerstreet/woodling": "~0.1.6",
		"barryvdh/laravel-ide-helper": "~1.9"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/modules",
			"public/addons"
		]
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
		},
		"scripts": {
			"post-install-cmd": [
			"php artisan optimize"
			],
			"post-update-cmd": [
			"php artisan ide-helper:generate",
			"php artisan optimize"
			]
			},
			"config": {
				"preferred-install": "dist"
			}
		}
